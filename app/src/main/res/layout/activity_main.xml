<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.sample.demo.csci342a1.MainActivity"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"
        >

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="100dp"
            android:background="#000000"
            android:id="@+id/View1"></com.sample.demo.csci342a1.TileView>

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:layout_marginLeft="10dp"
            android:background="#000000"
            android:id="@+id/View2"></com.sample.demo.csci342a1.TileView>

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:layout_marginLeft="10dp"
            android:background="#000000"
            android:id="@+id/View3"></com.sample.demo.csci342a1.TileView>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"
        android:layout_marginTop="10dp">

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:id="@+id/View4" />

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View5"></com.sample.demo.csci342a1.TileView>

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View6" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"
        android:layout_marginTop="10dp" >

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:id="@+id/View7" />

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View8" />

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View9" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.4"
        android:layout_marginTop="10dp" >

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:id="@+id/View10" />

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View11" />

        <com.sample.demo.csci342a1.TileView
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="0.3"
            android:background="#000000"
            android:layout_marginLeft="10dp"
            android:id="@+id/View12" />
    </LinearLayout>

</LinearLayout>
